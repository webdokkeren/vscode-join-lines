{
	"name": "joinlines",
	"displayName": "JoinLines",
	"description": "Adds join lines command to VS Code",
	"version": "0.0.1",
	"publisher": "Thor Madsen-Holm",
	"engines": {
		"vscode": "^0.10.1"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.joinLines"
	],
	"main": "./out/src/extension",
	"contributes": {
		"commands": [{
			"command": "extension.joinLines",
			"title": "Join lines"
		}],
        "keybindings": [
			{
				"command": "extension.joinLines",
				"key": "Ctrl+j"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "node ./node_modules/vscode/bin/compile",
		"compile": "node ./node_modules/vscode/bin/compile -watch -p ./"
	},
	"devDependencies": {
		"typescript": "^1.6.2",
		"vscode": "0.10.x"
	}
}